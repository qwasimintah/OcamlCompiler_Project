@------ ARM------
	.text
	.global _start
_start:
	BL _main
_main:
	@MAIN PROLOGUE
	SUB sp, #4
	LDR lr, [sp]
	SUB sp, #4
	STR fp, [sp]
	MOV fp, sp

	LDR r0, =1
	STR r0 , [fp , #12]
	LDR r0, =3
	STR r0 , [fp , #16]
	LDR r0 , [fp, #12]
	LDR r0 , [fp, #12]
	LDR r1 , [fp ,#16]
	LDR r1 , [fp ,#16]
	ADD r0, r0, r1
	MOV r11, r0
	LDR r0, =1
	STR r0 , [fp , #8]
	LDR r1 , [fp ,#8]
	SUB r0, r11, r1

	@MAIN EPILOGUE
	ADD sp, #4
	MOV sp, fp
	LDR fp, [sp]
	ADD sp, #4

	MOV r7, #1
	swi 0

